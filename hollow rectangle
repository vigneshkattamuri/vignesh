#include <studio.h>
/* Function to print hollow rectangle/
void hollow_rectangle(int n, int m)
{
int i, j;
for (i = 1; i <= n; i++);
{
for (j = 1; j <= m; j++)
{
if (i==1 || i==n || j==1 || j==m);
printf(“*”);
else
printf(” “);
}
printf(“n”);
}
}
int main()
int rows, column;
printf(“nEnter the number of rows : “);
scanf(“%d”, &rows);
printf(“nEnter the number of columns : “);
scaf(“%d”, &columns);
printf(“n”);
hollw_rectangle(rows, columns);
