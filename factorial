#include <stdio.h>
int main() {
    int n, i;
    unsigned fact = 1;
    printf("Enter an integer: ");
    scanf("%d", &n);

    
    if (n < 0)
        printf("Error! Factorial  negative num doesn't exist.");
    else {
        for (i = 1; i <= n; ++i) {
            fact *= i;
        }
        printf("Factorial of %d = %u", n, fact);
    }

    return 0;
}
