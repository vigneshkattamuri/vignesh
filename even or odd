#include <stdio.h>
 
int main()
{
 int i,last;

 printf("Enter The Last Number : ");
 scanf("%d",&last);


 printf("\nEven Number List :\n ");
 
 i=2;
 while(i <= last)
 {
  printf(" %d",i);
  i = i + 2;
 }

 
 printf("\nOdd Number List :\n ");

 i=1;
 while(i <= last)
 {
  printf(" %d",i);
  i = i + 2;
 }
}
